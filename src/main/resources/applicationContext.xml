<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="proxyPrinter" class="com.googlecode.arcane.proxy.ProxyPrinter">
        <constructor-arg name="cardBorderParam" value="0"/>
        <constructor-arg name="heightParam" value="319"/>
        <constructor-arg name="widthParam" value="222"/>
        <constructor-arg name="cardListWidthParam" value="11"/>
        <constructor-arg name="outputTemplateParam" value="proxy-template.ftl"/>
    </bean>

</beans>
